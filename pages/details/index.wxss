.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0 0;
  box-sizing: border-box;
  color: #fff;
}
.container .title {
	font-size: 110rpx;
}
.container .intro,
.container .error {
	font-size: 28rpx;
	width: 80%;
	text-align: center;
	margin-top: 20rpx;
	margin-bottom: 60rpx;
	color: #aaa;
}
.container .intro .check, 
.container .intro .company, 
.container .intro .number {
	margin: 20rpx 0;
} 
.container .details-wrapper {
	width: 80%;
	height: 600rpx;
	overflow-y: auto;
}
.container .details {
	width: 100%;
	text-align: left;
	margin: 28rpx 0;
	font-size: 24rpx;
	color: #999;
}
.container .details.cur {
	color: #30ad64;
}
.container .details .context{
	font-size: 28rpx;
}
